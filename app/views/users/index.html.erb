<div class="page-header">
<h2>Users Index</h2>
</div>

<br><br>
  <table class = "table table-striped">
    <tr>
      <th class = "col-md-3">
        Name
      </th>

      <th>
        Email
      </th>
    </tr>
    <% @users.each do |user| %>
    <tr>
      <td>
        <%= link_to user.full_name, user_path(user) %>
      </td>
      <td>
        <%= user.email %>
      </td>
    <% end %>
    </tr>
  </table>
